{
  "compileOnSave": false,
  "angularCompilerOptions": {
  },
  "compilerOptions": {
    "baseUrl": "./",
    "downlevelIteration": true,
    "importHelpers": true,
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "es2020",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ],
    "paths": {
      "@popetech/ngx-table": [
        "dist/ngx-table"
      ],
      "@popetech/ngx-table/*": [
        "dist/ngx-table/*"
      ]
    }
  }
}
